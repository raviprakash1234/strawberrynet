    <style>

body {
    font-family: Lato, sans-serif;
    height: 100%;
    width: 100%;
    margin: 0;
    
}
/* #header-cont{
    width: 77%;
} */

#top-cont {
    width: 100%;
    height: 100px;
    display: flex;
    background: url("https://a.cdnsbn.com/images/common/2016headerbg.jpg?v=2")
        top center no-repeat;
    /* border: 1px solid red; */
}

#strawberry-cont {
    float: left;
    width: 18%;
    padding: 20px 20px 20px 20px;
    padding-left: 50px;
    margin-left: 10%;
    /* border: 1px solid red; */
}

#search-cont {
    text-align: center;
    width: 40%;
}

#search-cont > input {
    border: 2px solid purple;
    width: 50%;
    height: 30%;
    margin-top: 25px;
    border-radius: 12px;
}

#search-cont > button {
    border: 2px solid purple;
    height: 36%;
    border-radius: 12px;
    color: #623381;
}

#right-cont {
    display: flex;
    flex-direction: row-reverse;
    width: 15%;
}

#right-cont > a {
    margin-right: 40px;
    margin-top: 8%;
    color: #b53788;
    font-size: 12px;
    text-align: center;
    text-decoration: none;
}

#right-cont svg {
    padding: 4px;
}

.special-item .cut {
    text-decoration: line-through;
}

.menu-cont {
    background-color: #623381;
    padding: 10px;
    text-align: center;
    /* border: 1px solid green; */
}

.button > a {
    color: white;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 18px;
    padding-bottom: 14px;
    font-size: 12px;
    font-weight: bold;
    text-decoration: none;
    letter-spacing: 0.03em;
}

.btn-hover > a:hover {
    background-color: white;
    color: #623381;
    text-decoration: underline;
}        
        .banner>img {
    height: 2%;
    margin-top: 1%;
    margin-bottom: 1%;
    float: right;

}

.b-cat>button {
    padding: 5px;
    color: #623381;
    border: 1px solid #623381;
    border-radius: 5px;
    margin-top: 0.5%;
    margin-right: 2%;
    margin-bottom: 0.5%;
    letter-spacing: 0.05em;
}

@media screen and (min-width: 600px) {
    #template {
        grid-template: 'tbox tbox tbox tbox'
            'lbox rbox rbox rbox';
    }
}

#template {
    display: grid;
    gap: 20px;
    margin-left: 3%;
    margin-right: 3%;
    margin-top: 2%;
}

#top {
    grid-area: tbox;
}

#top .banner img {
    width: 50%;
}

#top a {
    text-decoration: none;
    color: #ef1275;
    font-size: 12px;
}

#top h1 {
    letter-spacing: 0.3em;
}

#left {
    grid-area: lbox;
}

.t1 {
    margin-left: 20px;
}

#l2 {
    overflow-y: scroll;
    height: 200px;
}

#left h3 {
    color: #ef1275;
    letter-spacing: 0.1em;
}

#right {
    grid-area: rbox;
}

#right .rd1 td {
    padding: 10px;
    width: 300px;

}

#right .rd1 table {
    background-color: #70378d;
    ;
}

#right .rd1 a {
    text-decoration: none;
    color: white;
}

#right .rd1 .all-b a {
    background-color: white;
    color: black;
    border-radius: 6px;
    padding: 6px;
    font-size: 14px;
    font-weight: bold;
}

#right .rd1 th {
    padding: 10px;
    background-color: #52246d;
    color: white;
    font-weight: 700;
    font-size: 18px;
}

#right .rline {
    height: 1px;
    background-color: gray;
    margin: 40px 0px;
}

#right .rd2 th {
    background: linear-gradient(to right, #623381 30%, #c62f7a 100%);
    font-size: 18px;
    font-weight: 700;
    color: white;
    text-align: left;
    padding: 5px;
}

#right .rd2 .info {
    width: 40%;
    font-size: 16px;
    font-weight: 700;
    letter-spacing: 0.03em;
    padding: 20px;
}

#right .rd2 .info a {
    text-decoration: none;
    color: black;
}

#right .rd2 .pic {
    width: 30%;
    text-align: center;
    line-height: 1.2;
    padding: 20px;
}

#right .rd2 .pic img {
    width: 70px;
    height: 80px;
}

#right .rd2 .pic p {
    color: #662d91;
    font-size: 10px;
}

#right .rd2 .pic a {
    background-color: #f37021;
    font-size: 12px;
    padding: 5px;
    font-weight: 700;
    padding-left: 30px;
    padding-right: 30px;
    color: white;
    border-radius: 3px;
    text-decoration: none;
}

#right .rd2 .pr {
    width: 20%;
    text-align: center;
    background: url(https://a.cdnsbn.com/images/common/oitem_bg.gif);
    line-height: 0.3em;
}

#right .rd2 .pr h2 {
    color: white;
}

#right .rd2 .pr h5 {
    color: #ff0;
}

#right .rd2 .v {
    text-align: center;
    padding: 5px;
}

#right .rd2 .v a {
    text-decoration: none;
    background-color: #00a69c;
    font-size: 10px;
    text-transform: uppercase;
    padding: 3px;
    color: white;
    border-radius: 3px;
    padding-left: 10px;
    padding-right: 10px;
}
    </style>
</head>

